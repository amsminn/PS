#include <bits/stdc++.h>
using namespace std;

struct P{ int v, w; }; 
bool operator <(P a, P b){ return a.w > b.w;}

int T;
int n,m,t;
int s, g, h;
vector<int> sdist, gdist, hdist;
vector<P> v[2020];
vector<int> lst;

void init(){
	for(int i = 0; i<2019; i++) v[i].clear();
	lst.clear();
	sdist.clear();
	gdist.clear();
	hdist.clear();
	sdist.resize(n+5);
	gdist.resize(n+5);
	hdist.resize(n+5);
}

void djkstra(vector<int> &dist, int start){
	priority_queue<P> pq;
	dist[start] = 0;
	pq.push({start, 0});
	while(pq.size()){
		P cur = pq.top(); pq.pop();
		if(cur.w > dist[cur.v]) continue;	
		for(auto next : v[cur.v]){
			if(dist[next.v] > dist[cur.v] + next.w){
				dist[next.v] = dist[cur.v] + next.w;
				pq.push({next.v, dist[next.v]});
			}
		}
	}
}

int main(){
	cin.sync_with_stdio(0); cin.tie(0);
	cin>>T;
	while(T--){
		init();
		cin>>n>>m>>t;
		cin>>s>>g>>h;
		for(int i = 0; i<m; i++){
			int a,b,d; cin>>a>>b>>d;
			v[a].push_back({b,d});
			v[b].push_back({a,d});
		}
		for(int i = 0; i<t; i++){ 
			int a; cin>>a;
			lst.push_back(a);
		} 
		sort(lst.begin(), lst.end());
		djkstra(sdist, s); djkstra(gdist, g); djkstra(hdist, h);
		for(auto i : lst){
			if(sdist[i] == sdist[s] + gdist[h] + hdist[i])
				cout<<i<<' ';
		}
		cout<<"case end \n";
	}

	return 0;
}
